programa
{
	
	funcao inicio()
	{
		inteiro j, i, c, n
		logico a

		escreva("Informe o numero de testes: ")
		leia(n)

		para (c = 0; c < n; c++){
			escreva("\n\nInforme um numero: ")
			leia(i)
			
			a = verdadeiro
			para ( j = 2; j < i; j++ ) {
				se ( i % j == 0 )
					a = falso
				aparar
			}
			mar)
				escreva(i, "é Primo!")
			senao
				escreva (i, "é Não Primo!")
		}
	}
}
